        <activity
            android:name=".p5_picture_frame.PictureFrameActivity"
            android:theme="@style/Theme.ModuleBasic.NoActionBar"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>