            android:id="@+id/ott_view_curation_green"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_blue"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleY="1"
            android:scaleX="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintVertical_bias="0.7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_white"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>



    <ConstraintSet android:id="@+id/curation_animation_start2">
        <Constraint android:id="@+id/widget" />
        <Constraint
            android:id="@+id/ott_view_curation_red"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_yellow"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintVertical_bias="0.3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_green"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_blue"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
