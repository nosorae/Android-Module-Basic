<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Transition
        app:constraintSetEnd="@+id/curation_animation_end1"
        app:constraintSetStart="@+id/curation_animation_start1"
        app:duration="500"
        app:motionInterpolator="easeIn"/>

    <Transition
        app:constraintSetEnd="@+id/curation_animation_end2"
        app:constraintSetStart="@+id/curation_animation_start2"
        app:duration="500"
        app:motionInterpolator="easeIn"/>


    <ConstraintSet android:id="@+id/curation_animation_start1">
        <Constraint android:id="@+id/widget" />
        <Constraint
            android:id="@+id/ott_view_curation_red"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_yellow"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintVertical_bias="0.3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_green"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_blue"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintVertical_bias="0.7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_white"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/curation_animation_end1">
        <Constraint android:id="@id/widget" />
        <Constraint
            android:id="@+id/ott_view_curation_red"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintVertical_bias="0.3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ott_view_curation_yellow"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintVertical_bias="0.3"
            app:layout_constraintBottom_toBottomOf="parent"
